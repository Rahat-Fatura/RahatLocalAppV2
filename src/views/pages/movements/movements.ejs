<%- include('../../partials/doctype.ejs') %>

<head>
    <%- include('../../partials/header') %>
    <title><%= page.display %> - <%= app.name %></title>
    <%- include('../../partials/theme_css') %>
</head>

<body>
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-navbar-full layout-horizontal layout-without-menu">
        <div class="layout-container">
            <!-- Navbar -->
            <%- include('../../partials/navbar') %>
            <!-- / Navbar -->

            <!-- Layout container -->
            <div class="layout-page">
                <!-- Content wrapper -->
                <div class="content-wrapper">
                    <!-- Menu -->
                    <%- include('../../partials/menu') %>
                    <!-- / Menu -->
                    <!-- Content -->
                    <div class="container-xxl flex-grow-1 container-p-y">
                        <div class="card" id="table-div">
                            <div class="card-body demo-vertical-spacing demo-only-element">
                                <div class="row col-12">
                                    <div class="col-12 col-md-2 mt-1">
                                        <label class="form-label" for="searchbox">Ara</label>
                                        <div class="input-group">
                                            <span class="input-group-text"><i class="ti ti-search"></i></span>
                                            <input id="searchbox" type="text" class="form-control" placeholder="Ara...">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-2 mt-1">
                                        <label class="form-label" for="fdate">Başlangıç Tarihi</label>
                                        <div class="input-group">
                                            <span class="input-group-text"><i style="font-size: 0.8rem;" class="fa-regular fa-calendar-days fa-xs"></i></span>
                                            <input id="fdate" type="text" class="form-control" placeholder="Başlangıç Tarihi">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-2 mt-1">
                                        <label class="form-label" for="ldate">Bitiş Tarihi</label>
                                        <div class="input-group">
                                            <span class="input-group-text"><i style="font-size: 0.8rem;" class="fa-regular fa-calendar-days fa-xs"></i></span>
                                            <input id="ldate" type="text" class="form-control" placeholder="Bitiş Tarihi">
                                        </div>
                                    </div>
                                    <div class="col-12 col-md-2 mt-1">
                                        <label for="status" class="form-label">Durumu</label>
                                        <select id="status" class="form-select">
                                            <option></option>
                                            <option value="200">Başarılı</option>
                                            <option value="400">Hatalı</option>
                                            <option value="100">İşleniyor</option>
                                        </select>
                                    </div>
                                    <div class="col-12 col-md-2 mt-1">
                                        <label for="type-filter" class="form-label">Tipi</label>
                                        <select id="type-filter" class="form-select">
                                            <option></option>
                                            <option value="INSERT">INSERT</option>
                                            <option value="UPDATE">UPDATE</option>
                                            <option value="DELETE">DELETE</option>
                                            <option value="MANUAL">MANUAL</option>
                                            <option value="CHECKED">CHECKED</option>
                                            <option value="NOTIFY">NOTIFY</option>
                                            <option value="NOTIFY_INV_UPD">NOTIFY_INV_UPD</option>
                                            <option value="NOTIFY_INV_RFR">NOTIFY_INV_RFR</option>
                                            <option value="NOTIFY_DSP_UPD">NOTIFY_DSP_UPD</option>
                                            <option value="NOTIFY_DSP_RFR">NOTIFY_DSP_RFR</option>
                                        </select>
                                    </div>
                                    <div class="col-12 col-md-2 mt-1">
                                        <label for="document-filter" class="form-label">Belge</label>
                                        <select id="document-filter" class="form-select">
                                            <option></option>
                                            <option value="INVOICE">FATURA</option>
                                            <option value="DESPATCH">İRSALİYE</option>
                                        </select>
                                    </div>
                                    <div class="col-12 mt-md-1 mt-2 d-grid pt-4">
                                        <button id="search-button" type="button" class="btn btn-info waves-effect"><i class="fa-solid fa-magnifying-glass me-2"></i>Ara</button>
                                    </div>
                                </div>
                            </div>
                            <div class="card-datatable dataTable_select text-nowrap table-responsive">
                                <table class="table" id="documents">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Durum</th>
                                            <th>JSON</th>
                                            <th>ErpID</th>
                                            <th>ERP No</th>
                                            <th>detail</th>
                                            <th>Oluşturulma</th>
                                            <th>Güncellenme</th>
                                            <th>İşlem</th>
                                            <th>Belge</th>
                                        </tr>
                                    </thead>

                                </table>
                            </div>
                        </div>
                    </div>
                    <!-- / Content -->
                    <!-- Footer -->
                    <%- include('../../partials/footer') %>
                    <!-- / Footer -->

                    <div class="content-backdrop fade"></div>
                </div>
                <!-- Content wrapper -->
            </div>
            <!-- / Layout page -->
        </div>
        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>
        <!-- Drag Target Area To SlideIn Menu On Small Screens -->
        <div class="drag-target"></div>
    </div>
    <%- include('../../partials/theme_js') %>
</body>

</html>
